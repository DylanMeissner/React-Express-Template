{
  "name": "react-express-starter",
  "version": "1.0.0",
  "description": "A React project template bundled by Webpack and served with an Express server",
  "scripts": {
    "start": "ts-node server/index.ts",
    "dev": "webpack --mode development && open http://localhost:3000/ && ts-node server/index.ts ",
    "test": "jest",
    "test:watch": "jest --watch",
    "build": "webpack --mode=production --node-env=production",
    "build:dev": "rm -rf dist/ && webpack --mode=development",
    "build:prod": "rm -rf dist/ && webpack --mode=production --node-env=production"
  },
  "keywords": [],
  "author": "Dylan Meissner",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.22.7",
    "@babel/preset-env": "^7.22.7",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@testing-library/react": "^14.0.0",
    "@types/dotenv": "^8.2.0",
    "@types/ejs": "^3.1.2",
    "@types/express": "^4.17.17",
    "@types/file-loader": "^5.0.1",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.2.5",
    "@types/prettier": "^2.7.3",
    "@types/react": "^18.2.9",
    "@types/react-dom": "^18.2.4",
    "@types/sass-loader": "^8.0.5",
    "@types/webpack-env": "^1.18.1",
    "@types/webpack-hot-middleware": "^2.25.6",
    "autoprefixer": "^10.4.13",
    "babel-jest": "^29.6.0",
    "babel-loader": "^9.1.2",
    "css-loader": "^6.8.1",
    "dotenv": "^16.1.4",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.1",
    "jest": "^29.6.0",
    "jest-environment-jsdom": "^29.6.0",
    "mini-css-extract-plugin": "^2.7.0",
    "postcss": "^8.4.24",
    "postcss-loader": "^7.0.1",
    "prettier": "^2.6.2",
    "sass": "^1.63.2",
    "sass-loader": "^13.2.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "webpack": "^5.86.0",
